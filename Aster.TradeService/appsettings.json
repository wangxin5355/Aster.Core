{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Error",
      "Microsoft": "Error"
    }
  },
  "ConnectionStrings": {
    "MySqlConnectionString": "datasource=127.0.0.1;port=3306;Database=aster;user=root;pwd=root;SslMode=None;pooling=true;Charset=utf8"
  },
  "Redis": {
    "Servers": [ "127.0.0.1:6379,poolsize=5,writeBuffer=20240" ],
    "KeyPreffix": "aster:trade:"
  },
  "TokenOptions": {
    "IpWhiteList": [ "127.0.0.1", "localhost", "::1" ]
  },
  "Localization": {
    "DefaultCulture": "zh-CN",
    "SupportedCultures": [ "en", "zh-CN", "ko-KR", "ja-JP" ],
    "CreateNewRecordWhenLocalisedStringDoesNotExist": true,
    "DbConnectionString": "datasource=127.0.0.1;port=3306;Database=aster;user=root;pwd=root;SslMode=None;pooling=true;Charset=utf8;Allow User Variables=True;AllowPublicKeyRetrieval=True"
  },
  "AllowedHosts": "*",
  /*服务治理第三方组件Consul相关配置参数*/
  "ServiceDiscovery": {
    "ServiceName": "TradeService", //本WebApi项目的服务名称，可以随便取名，将显示Consul的管理页面上，届时通过该服务名查找可用的服务站点
    "Consul": {
      "HttpEndpoint": "http://127.0.0.1:8500",
      "TcpEndpoint": {
        "Address": "127.0.0.1",
        "Port": 8600
      }
    }
  }
}
